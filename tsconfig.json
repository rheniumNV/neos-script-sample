{
  "compilerOptions": {
    "target": "ES5",
    "module": "CommonJS",
    "jsx": "react-jsx",
    "jsxImportSource": "neos-script",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "allowJs": true,
    "checkJs": false,
    "rootDir": "src",
    "outDir": "dist",
    "strict": true
  }
}
